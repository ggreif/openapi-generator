{
  "openapi": "3.0.0",
  "info": {
    "title": "Type Coverage Test API",
    "version": "1.0.0",
    "description": "Comprehensive test API for all Motoko type mappings"
  },
  "servers": [
    {
      "url": "https://example.com"
    }
  ],
  "paths": {
    "/all-types": {
      "get": {
        "operationId": "getAllTypes",
        "summary": "Get object with all basic types",
        "responses": {
          "200": {
            "description": "Object containing all supported types",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AllTypes"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "postAllTypes",
        "summary": "Send AllTypes object in request body",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AllTypes"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Success, no content returned"
          }
        }
      }
    },
    "/matrix": {
      "get": {
        "operationId": "getMatrix",
        "summary": "Get nested array (2D matrix)",
        "responses": {
          "200": {
            "description": "A 2D array of integers",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "postMatrix",
        "summary": "Send nested array in request body",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "array",
                  "items": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/arrays": {
      "get": {
        "operationId": "getArrays",
        "summary": "Get object with various array types",
        "responses": {
          "200": {
            "description": "Object with different array types",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ArrayTypes"
                }
              }
            }
          }
        }
      }
    },
    "/user/{id}": {
      "get": {
        "operationId": "getUserById",
        "summary": "Get user by ID with query parameters",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "includeMetadata",
            "in": "query",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User object",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          }
        }
      }
    },
    "/map-test": {
      "get": {
        "operationId": "getMapTest",
        "summary": "Test model named 'Map' (name collision with Map<K,V>)",
        "responses": {
          "200": {
            "description": "Custom Map model and actual map type",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MapTest"
                }
              }
            }
          }
        }
      }
    },
    "/reserved-words": {
      "get": {
        "operationId": "getReservedWords",
        "summary": "Test model with fields named after Motoko reserved words",
        "responses": {
          "200": {
            "description": "Object with reserved word field names",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReservedWords"
                }
              }
            }
          }
        }
      }
    },
    "/enums": {
      "get": {
        "operationId": "getEnums",
        "summary": "Test enum handling in request and response",
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "schema": {
              "$ref": "#/components/schemas/Status"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Enum container response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EnumContainer"
                }
              }
            }
          }
        }
      }
    },
    "/matrix/arrays": {
      "post": {
        "operationId": "testArrayMatrix",
        "summary": "Test all array container types: Array<T> for various T",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ArrayMatrix"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Echo back the array matrix",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ArrayMatrix"
                }
              }
            }
          }
        }
      }
    },
    "/matrix/maps": {
      "post": {
        "operationId": "testMapMatrix",
        "summary": "Test all map container types: Map<String, T> for various T",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MapMatrix"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Echo back the map matrix",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MapMatrix"
                }
              }
            }
          }
        }
      }
    },
    "/matrix/optionals": {
      "post": {
        "operationId": "testOptionalMatrix",
        "summary": "Test optional versions of container types",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OptionalMatrix"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Echo back the optional matrix",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OptionalMatrix"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AllTypes": {
        "type": "object",
        "required": [
          "textField",
          "boolField",
          "intField",
          "floatField"
        ],
        "properties": {
          "textField": {
            "type": "string"
          },
          "charField": {
            "type": "string",
            "maxLength": 1
          },
          "boolField": {
            "type": "boolean"
          },
          "intField": {
            "type": "integer"
          },
          "longField": {
            "type": "integer",
            "format": "int64"
          },
          "floatField": {
            "type": "number",
            "format": "float"
          },
          "doubleField": {
            "type": "number",
            "format": "double"
          },
          "dateField": {
            "type": "string",
            "format": "date"
          },
          "dateTimeField": {
            "type": "string",
            "format": "date-time"
          },
          "passwordField": {
            "type": "string",
            "format": "password"
          },
          "binaryField": {
            "type": "string",
            "format": "binary"
          },
          "uuidField": {
            "type": "string",
            "format": "uuid"
          },
          "uriField": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "ArrayTypes": {
        "type": "object",
        "required": ["strings", "integers", "nested"],
        "properties": {
          "strings": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "integers": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "floats": {
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "nested": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      },
      "User": {
        "type": "object",
        "required": ["id", "name", "email"],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "age": {
            "type": "integer"
          },
          "isActive": {
            "type": "boolean"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "Map": {
        "type": "object",
        "required": ["key", "value"],
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "MapTest": {
        "type": "object",
        "required": ["customMap", "metadata"],
        "properties": {
          "customMap": {
            "$ref": "#/components/schemas/Map"
          },
          "metadata": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          }
        }
      },
      "ReservedWords": {
        "type": "object",
        "required": ["actor", "async", "try", "type"],
        "properties": {
          "actor": {
            "type": "string",
            "description": "Motoko keyword for actor declaration"
          },
          "and": {
            "type": "boolean",
            "description": "Motoko boolean operator"
          },
          "async": {
            "type": "boolean",
            "description": "Motoko keyword for async functions"
          },
          "await": {
            "type": "string",
            "description": "Motoko keyword for awaiting async calls"
          },
          "try": {
            "type": "integer",
            "description": "Motoko keyword for exception handling"
          },
          "catch": {
            "type": "string",
            "description": "Motoko keyword for exception handling"
          },
          "throw": {
            "type": "string",
            "description": "Motoko keyword for throwing errors"
          },
          "import": {
            "type": "string",
            "description": "Motoko keyword for importing modules"
          },
          "module": {
            "type": "string",
            "description": "Motoko keyword for module declaration"
          },
          "not": {
            "type": "boolean",
            "description": "Motoko boolean operator"
          },
          "null": {
            "type": "string",
            "description": "Motoko null literal"
          },
          "or": {
            "type": "boolean",
            "description": "Motoko boolean operator"
          },
          "type": {
            "type": "string",
            "description": "Motoko keyword for type declaration"
          },
          "class": {
            "type": "string",
            "description": "Motoko keyword for class declaration"
          },
          "func": {
            "type": "string",
            "description": "Motoko keyword for function declaration"
          },
          "public": {
            "type": "boolean",
            "description": "Motoko keyword for public visibility"
          },
          "private": {
            "type": "boolean",
            "description": "Motoko keyword for private visibility"
          },
          "shared": {
            "type": "boolean",
            "description": "Motoko keyword for shared functions"
          },
          "query": {
            "type": "string",
            "description": "Motoko keyword for query functions"
          },
          "let": {
            "type": "integer",
            "description": "Motoko keyword for immutable binding"
          },
          "var": {
            "type": "integer",
            "description": "Motoko keyword for mutable binding"
          },
          "if": {
            "type": "boolean",
            "description": "Motoko keyword for conditional"
          },
          "in": {
            "type": "string",
            "description": "Motoko keyword for iteration"
          },
          "else": {
            "type": "string",
            "description": "Motoko keyword for conditional"
          },
          "false": {
            "type": "boolean",
            "description": "Motoko boolean literal"
          },
          "switch": {
            "type": "string",
            "description": "Motoko keyword for pattern matching"
          },
          "case": {
            "type": "string",
            "description": "Motoko keyword for pattern matching"
          },
          "for": {
            "type": "integer",
            "description": "Motoko keyword for loops"
          },
          "while": {
            "type": "boolean",
            "description": "Motoko keyword for loops"
          },
          "loop": {
            "type": "string",
            "description": "Motoko keyword for infinite loops"
          },
          "return": {
            "type": "string",
            "description": "Motoko keyword for returning values"
          },
          "break": {
            "type": "string",
            "description": "Motoko keyword for loop control"
          },
          "continue": {
            "type": "string",
            "description": "Motoko keyword for loop control"
          },
          "label": {
            "type": "string",
            "description": "Motoko keyword for labeled expressions"
          },
          "debug": {
            "type": "boolean",
            "description": "Motoko keyword for debugging"
          },
          "do": {
            "type": "string",
            "description": "Motoko keyword for do-while loops"
          },
          "assert": {
            "type": "boolean",
            "description": "Motoko keyword for assertions"
          },
          "system": {
            "type": "string",
            "description": "Motoko keyword for system functions"
          },
          "true": {
            "type": "boolean",
            "description": "Motoko boolean literal"
          },
          "with": {
            "type": "string",
            "description": "Motoko keyword for with expressions"
          },
          "object": {
            "type": "string",
            "description": "Motoko keyword for object literals"
          }
        }
      },
      "Status": {
        "type": "string",
        "description": "Basic enum test",
        "enum": ["available", "pending", "sold"]
      },
      "Priority": {
        "type": "string",
        "description": "Reserved word collision test",
        "enum": ["low", "medium", "high", "async", "type", "null"]
      },
      "Color": {
        "type": "string",
        "description": "Special characters test",
        "enum": ["red", "blue-green", "dark gray", "#FF00FF"]
      },
      "NumericCode": {
        "type": "integer",
        "description": "Numeric enum - should remain Int",
        "enum": [100, 200, 404, 500]
      },
      "EnumContainer": {
        "type": "object",
        "required": ["status"],
        "properties": {
          "status": {
            "$ref": "#/components/schemas/Status"
          },
          "priority": {
            "$ref": "#/components/schemas/Priority"
          },
          "color": {
            "$ref": "#/components/schemas/Color"
          },
          "code": {
            "$ref": "#/components/schemas/NumericCode"
          },
          "simpleText": {
            "type": "string",
            "description": "Simple text field (inline enums to be implemented later)"
          },
          "statusArray": {
            "type": "array",
            "description": "Array of enum values",
            "items": {
              "$ref": "#/components/schemas/Status"
            }
          }
        }
      },
      "SimpleObject": {
        "type": "object",
        "description": "Simple object for use in matrix tests",
        "required": ["id", "name"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          }
        }
      },
      "ArrayMatrix": {
        "type": "object",
        "description": "Matrix test: Array<T> for all element types T",
        "required": [
          "arrayOfStrings",
          "arrayOfInts",
          "arrayOfBools",
          "arrayOfEnums",
          "arrayOfObjects",
          "arrayOfArrays",
          "arrayOfMaps"
        ],
        "properties": {
          "arrayOfStrings": {
            "type": "array",
            "description": "Array<Text>",
            "items": {
              "type": "string"
            }
          },
          "arrayOfInts": {
            "type": "array",
            "description": "Array<Int>",
            "items": {
              "type": "integer"
            }
          },
          "arrayOfBools": {
            "type": "array",
            "description": "Array<Bool>",
            "items": {
              "type": "boolean"
            }
          },
          "arrayOfEnums": {
            "type": "array",
            "description": "Array<Status>",
            "items": {
              "$ref": "#/components/schemas/Status"
            }
          },
          "arrayOfObjects": {
            "type": "array",
            "description": "Array<SimpleObject>",
            "items": {
              "$ref": "#/components/schemas/SimpleObject"
            }
          },
          "arrayOfArrays": {
            "type": "array",
            "description": "Array<Array<Int>> - 2D array",
            "items": {
              "type": "array",
              "items": {
                "type": "integer"
              }
            }
          },
          "arrayOfMaps": {
            "type": "array",
            "description": "Array<Map<Text, Int>>",
            "items": {
              "type": "object",
              "additionalProperties": {
                "type": "integer"
              }
            }
          },
          "arrayOf3D": {
            "type": "array",
            "description": "Array<Array<Array<Text>>> - 3D array",
            "items": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "MapMatrix": {
        "type": "object",
        "description": "Matrix test: Map<Text, T> for all value types T",
        "required": [
          "mapOfStrings",
          "mapOfInts",
          "mapOfBools",
          "mapOfEnums",
          "mapOfObjects",
          "mapOfArrays",
          "mapOfMaps"
        ],
        "properties": {
          "mapOfStrings": {
            "type": "object",
            "description": "Map<Text, Text>",
            "additionalProperties": {
              "type": "string"
            }
          },
          "mapOfInts": {
            "type": "object",
            "description": "Map<Text, Int>",
            "additionalProperties": {
              "type": "integer"
            }
          },
          "mapOfBools": {
            "type": "object",
            "description": "Map<Text, Bool>",
            "additionalProperties": {
              "type": "boolean"
            }
          },
          "mapOfEnums": {
            "type": "object",
            "description": "Map<Text, Status>",
            "additionalProperties": {
              "$ref": "#/components/schemas/Status"
            }
          },
          "mapOfObjects": {
            "type": "object",
            "description": "Map<Text, SimpleObject>",
            "additionalProperties": {
              "$ref": "#/components/schemas/SimpleObject"
            }
          },
          "mapOfArrays": {
            "type": "object",
            "description": "Map<Text, Array<Text>>",
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "mapOfMaps": {
            "type": "object",
            "description": "Map<Text, Map<Text, Int>> - nested maps",
            "additionalProperties": {
              "type": "object",
              "additionalProperties": {
                "type": "integer"
              }
            }
          }
        }
      },
      "OptionalMatrix": {
        "type": "object",
        "description": "Matrix test: Optional<T> for container types",
        "properties": {
          "optArrayOfStrings": {
            "type": "array",
            "description": "?Array<Text>",
            "items": {
              "type": "string"
            }
          },
          "optArrayOfEnums": {
            "type": "array",
            "description": "?Array<Status>",
            "items": {
              "$ref": "#/components/schemas/Status"
            }
          },
          "optArrayOfObjects": {
            "type": "array",
            "description": "?Array<SimpleObject>",
            "items": {
              "$ref": "#/components/schemas/SimpleObject"
            }
          },
          "optMapOfStrings": {
            "type": "object",
            "description": "?Map<Text, Text>",
            "additionalProperties": {
              "type": "string"
            }
          },
          "optMapOfEnums": {
            "type": "object",
            "description": "?Map<Text, Status>",
            "additionalProperties": {
              "$ref": "#/components/schemas/Status"
            }
          },
          "optMapOfObjects": {
            "type": "object",
            "description": "?Map<Text, SimpleObject>",
            "additionalProperties": {
              "$ref": "#/components/schemas/SimpleObject"
            }
          },
          "arrayOfOptStrings": {
            "type": "array",
            "description": "Array<?Text>",
            "items": {
              "type": "string",
              "nullable": true
            }
          },
          "arrayOfOptEnums": {
            "type": "array",
            "description": "Array<?Status>",
            "items": {
              "allOf": [
                {
                  "$ref": "#/components/schemas/Status"
                }
              ],
              "nullable": true
            }
          },
          "mapOfOptInts": {
            "type": "object",
            "description": "Map<Text, ?Int>",
            "additionalProperties": {
              "type": "integer",
              "nullable": true
            }
          }
        }
      }
    }
  }
}
